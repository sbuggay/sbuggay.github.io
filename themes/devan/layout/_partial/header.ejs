<header id="header">
  <div id="header-outer" class="outer">
    <div id="header-inner" class="inner">
      <nav id="main-nav">
        <div id="main-nav-links">
          <% for (var i in theme.menu) { %>
              <a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
          <% } %>
        </div>
        <div id="main-nav-social">
          <a class="main-nav-icon linkedin" href="https://www.linkedin.com/in/devanbuggay/">
            <i class="icon-linkedin-squared" aria-hidden="true"></i>
          </a>
          <a class="main-nav-icon github" href="https://github.com/sbuggay">
            <i class="icon-github-squared" aria-hidden="true"></i>
          </a>
        <div>
      </nav>
    </div>
  </div>
</header>