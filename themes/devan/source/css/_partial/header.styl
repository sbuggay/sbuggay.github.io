#header
  height: banner-height
  border-bottom: 1px solid color-border

#header-outer
  height: 100%

#header-inner
  height: 100%

#subtitle-wrap
  margin-top: subtitle-size

#main-nav
  display: flex
  justify-content: left
  height: 100%

$nav-link
  color: #fff
  text-decoration: none
  padding: 20px
  &:hover
    text-decoration: underline

.main-nav-link
  @extend $nav-link